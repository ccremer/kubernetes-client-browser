{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "lint",
          "build",
          "e2e",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "e2e": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "{projectRoot}/playwright-report"
      ]
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ]
    }
  }
}
