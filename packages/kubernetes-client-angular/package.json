{
  "name": "@ccremer/kubernetes-client-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "lint": "ng lint",
    "build": "for PROJ in $(ls projects); do ng build $PROJ; done",
    "watch": "ng build --watch --configuration development",
    "pree2e": "playwright install",
    "e2e": "playwright test",
    "prepack": "yq e -i '. += load(\"package.json\")' dist/kubernetes-client-angular/package.json -o json",
    "publish": "cd dist/kubernetes-client-angular && npm publish"
  },
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@ccremer/kubernetes-client": "*",
    "@ngrx/data": "^15.4.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "bootstrap": "5.2.3",
    "jose": "4.14.0",
    "rxjs": "~7.8.0",
    "tslib": "2.5.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.6",
    "@angular/compiler-cli": "^15.2.0",
    "@ngrx/store-devtools": "15.4.0",
    "@playwright/test": "^1.32.3",
    "eslint-config-custom": "*",
    "eslint-plugin-ngrx": "2.1.4",
    "ng-packagr": "^15.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist/kubernetes-client-angular"
  ],
  "types": "./dist/kubernetes-client-angular/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ccremer/kubernetes-client-browser"
  },
  "keywords": [
    "angular",
    "kubernetes",
    "client",
    "browser"
  ]
}
