{
  "name": "@nxt-engineering/kubernetes-client-svelte",
  "version": "0.0.1",
  "author": "nxt-engineering <admin@nxt.engineering>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nxt-engineering/kubernetes-client-browser.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nxt-engineering/kubernetes-client-browser/issues"
  },
  "homepage": "https://github.com/nxt-engineering/kubernetes-client-browser#readme",
  "scripts": {
    "dev": "vite dev",
    "prebuild": "npm run sass",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "sass": "sass --load-path=../../node_modules src/routes/styles.scss src/routes/styles.css"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^3.59.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "1.18.0",
    "@sveltejs/package": "2.0.2",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "bootstrap": "5.2.3",
    "eslint": "8.41.0",
    "eslint-config-custom": "*",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte": "2.29.0",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.8.1",
    "publint": "0.1.12",
    "sass": "1.62.1",
    "svelte": "3.59.1",
    "svelte-check": "3.3.2",
    "tslib": "2.5.2",
    "typescript": "5.0.4",
    "vite": "4.3.8",
    "vitest": "0.31.1"
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "kubernetes",
    "client",
    "browser",
    "typescript",
    "svelte",
    "sveltekit"
  ]
}
